{
  "name": "Parroteer",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "No-code test automation solution for end-to-end testing",
  "homepage_url": "https://github.com/oslabs-beta/parroteer",
  "action": {
    "default_title": "Parroteer",
    "default_popup": "./popup.html"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "./content.js"
      ]
    }
  ]
}